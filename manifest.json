{
	"name": "X blocker",
	"description": "Blocks incoming things from the web.",
	"version": "1.0",

	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"*://*/*"
	],
	"background": {
		"scripts": [ "xblocker.helpers.js", "xblocker.background.js" ]
	},
	"options_page": "options/options.html",

	"icons": {
		"128": "images/128x128.png"
	},
	"manifest_version": 2
}
